<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="api.url" value="http://api.dotnetgroup.lt/v1/stream" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="Off" xdt:Transform="Insert" />
  </system.web>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" xdt:Transform="Insert" />
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" xdt:Transform="Insert" />
    </staticContent>
  </system.webServer>
</configuration>